#!/bin/bash
vpn=`naclient status | grep 'd\d\{1,\}p\d\{1,\}' | cut -f2 -d:`
if `echo $vpn | grep -q Int`; then
  if [ $vpn = 'd4p4-Int' ]; then
    ssh -q msean@172.20.132.3
  elif [ $vpn = 'd3p6-Int' ]; then
    ssh -q msean@172.20.134.3
  fi
else
  POD=`naclient status | grep 'd\d\{1,\}p\d\{1,\}' | cut -d'p' -f 2 | egrep  -o '\d'`
  ssh -q smurphy@172.20.$POD.3
fi
